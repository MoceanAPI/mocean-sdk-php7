{
  "name": "mocean/client7",
  "description": "PHP Client for using Mocean's API.",
  "license": "MIT",
  "type": "library",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.3 | ^8.0",
    "php-http/guzzle7-adapter": "^0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0 | ^9.0",
    "php-http/mock-client": "*",
    "phpspec/prophecy-phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "Mocean\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MoceanTest\\": "test/"
    }
  }
}

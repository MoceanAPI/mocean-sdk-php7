{
  "name": "mocean/client",
  "description": "PHP Client for using Mocean's API.",
  "license": "MIT",
  "type": "library",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.0.0 <8.0.0",
    "php-http/guzzle6-adapter": "^1.0 || ^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.0",
    "php-http/mock-client": "^1.4.0"
  },
  "autoload": {
    "psr-4": {
      "Mocean\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MoceanTest\\": "test/"
    }
  }
}
